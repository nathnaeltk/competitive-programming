def split_and_join(line):
    splitAndJoined = ""
    for char in line:
        if char != " ":
            splitAndJoined += char
        else:
            splitAndJoined += "-"
            
    return splitAndJoined
            

if __name__ == '__main__':
    line = input()
    result = split_and_join(line)
    print(result)
